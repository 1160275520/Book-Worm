<div class="container">
  <h1>My activity</h1>
</div>

<div class="container">
  <h3>Books that I shared</h3>
</div>

<div class="container">
  <div class="row">
    <% @profile.books_owned.each_with_index do |book,index| %>
        <%= render 'book_imformation', book: book, index: index %>
    <% end %> 
  </div>
</div>

<div class="container">
  <h3>Books that I borrowed</h3>
</div>


<div class="container">
  <div class="row">
    <% @profile.books_borrowed.each_with_index do |book,index| %>
        <%= render 'book_imformation', book: book, index: index %>
    <% end %> 
  </div>
</div>
<script type="text/javascript">
   var books=loadBooks();
</script>
